    {
      "education": []
    },
    {
      "health": []
    },
    {
      "housing": []
    },
    {
      "sport": []
    }
  ]
}



testing adding cost item
----------------------------------
url=http://localhost:3000/api/add/
data.status_code=201
b'{\n  "message": "Cost Item Added Successfully",\n  "data": {\n    "description": "milk 9",\n    "category": "food",\n    "userid": 123123,\n    "sum": 8,\n    "date": "2025-06-02T11:54:26.022Z",\n    "_id": "683d90f2b882cbea5776bebb",\n    "__v": 0\n  }\n}'


